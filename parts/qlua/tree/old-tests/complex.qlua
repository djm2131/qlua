-- simple real numbers
a = 1;
b = 2;

print("a=", a);
print("b=", b);

c = complex(3,4)
z = complex(5,6)
print("c:real()", c:real())
print("c:imag()", c:imag())
print("c*=", c:conj())
print("|c|=", c:abs())
print("c:norm2()", c:norm2())
print("c:abs()", c:abs())
print("c:arg()", c:arg())
print("c:sqrt()", c:sqrt())
print("c:exp()", c:exp())
print("c:log()", c:log())
print("-c=", -c)
print("c=", c)
print("z=", z)

print("z+c", z + c)
print("a+c", a + c)
print("z+b", z + b)

print("z-c", z - c)
print("a-c", a - c)
print("z-b", z - b)

print("z*c", z * c)
print("a*c", a * c)
print("z*b", z * b)

print("z/c*c", z / c * c)
print("a/c*c", a / c * c)
print("z/b*b", z / b * b)

print("6*c-4*z", 6 * c - z * 4)
